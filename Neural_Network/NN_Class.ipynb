    "def sigmoid(x):\n",
    "    return 1/float((1+np.exp(-x)))\n",
    "\n",
    "def sigmoidd(x):\n",
    "    sig=[]\n",
    "    for row in x:\n",
    "        r=[]\n",
    "        for value in row:\n",
    "            r.append(sigmoid(value))\n",
    "        sig.append(r)\n",
    "    return sig\n",
    "\n",
    "def diff_sigmoid(x):\n",
    "    return sigmoid(x)*(1-sigmoid(x))\n",
    "\n",
    "def diff_sigmoidd(x):\n",
    "    sig=[]\n",
    "    for row in x:\n",
    "        r=[]\n",
    "        for value in row:\n",
    "            r.append(diff_sigmoid(value))\n",
    "        sig.append(r)\n",
    "    return sig"
